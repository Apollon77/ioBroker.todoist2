{
  "common": {
    "name": "todoist2",
    "require": {
      "js-controller": ">0.6.6"
    },
    "version": "0.0.7",
    "news": {
      "0.0.7": {
        "en": "Update funktions",
        "de": "Neue Funktionen eingebaut"
      },
      "0.0.5": {
        "en": "Update funktions",
        "de": "Neue Funktionen eingebaut"
      },
      "0.0.1": {
        "en": "first release",
        "de": "erster Release"
      }
    },
    "title": "todoist2",
    "desc": {
      "en": "This adapter communikates with todoist",
      "de": "Adapter erm√∂glicht eine Kommunikation mit todoist"
    },
    "authors": [
      "rde-master <info@rde-master.de>"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "messagebox": true,
    "readme": "https://raw.githubusercontent.com/rde-master/ioBroker.todoist2/master/README.md",
    "loglevel": "info",
    "icon": "todoist.png",
    "materialize": true,
    "supportCustoms": true,
    "compact": true,
    "keywords": [
      "notification",
      "todoist",
      "message"
    ],
    "restartSchedule": "4 4 * * *",
    "extIcon": "https://raw.githubusercontent.com/rde-master/ioBroker.todoist2/master/admin/todoist.png",
    "type": "messaging",
    "stopBeforeUpdate": true,
    "blockly": true,
    "config": {
      "minWidth": 800,
      "width": 1224,
      "height": 600
    },
    "installedFrom": "https://github.com/rde-master/ioBroker.todoist2/tarball/76c1037f18679af8e6b8bce994225a780351fad3"
  },
  "native": {
    "token": "",
    "pollingInterval": 60000,
    "debug": true,
    "project": true,
    "section": false,
    "tasks": true,
    "labels": true,
    "dublicate": true,
    "text_objects": true,
    "json_objects": true,
    "html_objects": true
  },
  "objects": [
    {
      "_id": "_design/custom",
      "language": "javascript",
      "views": {
        "state": {
          "map": "function(doc) { if (doc.type === 'state' && (doc.common.custom || doc.common.history)) emit(doc._id, doc.common.custom || doc.common.history) }"
        }
      }
    }
  ],
  "instanceObjects": [
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "If connected to Telegram server",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}